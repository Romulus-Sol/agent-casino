{
  "name": "agent-casino",
  "version": "0.1.0",
  "description": "A headless casino protocol for AI agents on Solana",
  "private": true,
  "workspaces": [
    "sdk",
    "server"
  ],
  "scripts": {
    "build": "anchor build && cd sdk && npm run build",
    "test": "anchor test",
    "deploy:devnet": "anchor deploy --provider.cluster devnet",
    "deploy:mainnet": "anchor deploy --provider.cluster mainnet",
    "start:server": "cd server && npx ts-node index.ts",
    "example:degen": "npx ts-node examples/degen-agent.ts",
    "example:analyst": "npx ts-node examples/analyst-agent.ts",
    "example:house": "npx ts-node examples/house-agent.ts"
  },
  "devDependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/web3.js": "^1.95.0",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.0.0",
    "chai": "^4.3.7",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "solana",
    "casino",
    "ai-agents",
    "gambling",
    "defi",
    "anchor",
    "hackathon"
  ],
  "author": "Agent Casino",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Romulus-Sol/agent-casino"
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.14",
    "@switchboard-xyz/on-demand": "^3.8.2"
  }
}
